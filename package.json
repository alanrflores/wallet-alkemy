{
  "name": "aconcagua-backend",
  "version": "1.0.1",
  "description": "Earn time with this powerful, scalable and easy to config boilerplate.",
  "author": "gabodmc",
  "license": "MIT",
  "homepage": "https://github.com/gabodmc/nodejs-boilerplate#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/gabodmc/nodejs-boilerplate"
  },
  "scripts": {
    "dev": "nodemon src/app.js",
    "start": "node app.js",
    "lint": "eslint",
    "migrations:generate": "sequelize-cli migration:generate --name",
    "migrations:run": "sequelize-cli db:migrate",
    "migrations:revert": "sequelize-cli db:migrate:undo",
    "migrations:delete": "sequelize-cli db:migrate:undo:all",
    "seeds:generate": "npx sequelize-cli seed:generate --name",
    "seeds:run": "npx sequelize-cli db:seed:all",
    "test": "mocha ./test/*.js"
  },
  "engines": {
    "node": ">=0.14"
  },
  "dependencies": {
    "@hapi/boom": "^10.0.0",
    "bcrypt": "^5.1.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "~4.16.1",
    "express-validator": "^6.13.0",
    "http-errors": "~1.6.3",
    "http-status-codes": "^2.2.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "~1.9.1",
    "mysql2": "^2.3.3",
    "nodemailer": "^6.8.0",
    "sequelize": "^6.25.3",
    "sequelize-cli": "6.2.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "chai": "^4.3.4",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "27.0.6",
    "mocha": "^9.1.3",
    "nodemon": "^2.0.20",
    "prettier": "^2.3.2",
    "supertest": "^6.1.6"
  }
}
